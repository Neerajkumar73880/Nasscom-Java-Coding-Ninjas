import java.util.*;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner s = new Scanner(System.in);
        String str = s.next();
        char ch = str.charAt(0);
        int val=(int)ch;
        if(val>=65 && val<=90){
            System.out.println("1");
        }
        else if(val>=97 && val<=122){
            System.out.println("0");
        }
        else{
            System.out.println("-1");
            
        }

    }
}
