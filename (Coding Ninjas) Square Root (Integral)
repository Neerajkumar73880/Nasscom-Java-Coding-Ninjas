import java.util.*;
import java.lang.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner s = new Scanner(System.in);
        int num = s.nextInt();
        int i;
        int val=0;
        for(i=1;i*i<=num;i++){
                val = i;
        }
        System.out.println(val);

	}
}
